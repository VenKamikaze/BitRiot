<rml>
	<head>
		<title>Game Options</title>
		<link rel="stylesheet" type="text/css" href="rkt.rcss"/>
		<link rel="stylesheet" type="text/css" href="common.rcss"/>
		<link type="text/rcss" href="gameoptions.rcss"/>
	</head>
	<body class="background" onload="changebuttons">
		
    <div class="menu-container">
      <div class="menu">
	      <button id="totalplayers" autofocus tabindex="1" onclick="totalplayers">Total Players: </button><br />
	      <button id="humanplayers" tabindex="2" onclick="humanplayers">Human Players: </button><br />
	      <button id="genderplayer1" tabindex="3" onclick="genderplayer1">Player 1 Gender: </button><br />
	      <button id="genderplayer2" tabindex="4" onclick="genderplayer2">Player 2 Gender: </button><br />
	      <button id="genderplayer3" tabindex="5" onclick="genderplayer3">Player 3 Gender: </button><br />
	      <button id="genderplayer4" tabindex="6" onclick="genderplayer4">Player 4 Gender: </button><br />
	      <button id="blocktilepercent" tabindex="7" onclick="blocktilepercent">Block Coverage (%): </button><br />
	      <button id="mapwidth" tabindex="8" onclick="mapwidth">Map Width: </button><br />
	      <button id="mapheight" tabindex="9" onclick="mapheight">Map Height: </button><br />
	      <button id="back" tabindex="10" onclick="open mainmenu.rml">Back</button><br />
      </div>
    </div>
		
	</body>

</rml>
